{
  "name": "@signifly/datocms-backup-shared",
  "version": "1.0.0",
  "private": false,
  "description": "Shared types and constants for DatoCMS backup system",
  "homepage": "https://github.com/signifly/datocms-plugin-signifly-backups",
  "repository": {
    "type": "git",
    "url": "https://github.com/signifly/datocms-plugin-signifly-backups.git",
    "directory": "packages/shared"
  },
  "license": "MIT",
  "author": {
    "name": "Signifly",
    "url": "https://signifly.com"
  },
  "keywords": [
    "datocms",
    "backup",
    "types",
    "signifly"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
