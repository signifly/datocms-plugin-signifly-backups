{
  "name": "@signifly/datocms-plugin-signifly-backups",
  "version": "1.0.0",
  "private": false,
  "description": "DatoCMS plugin by Signifly for automatic environment backups with scheduled daily, weekly, and monthly snapshots",
  "homepage": "https://github.com/signifly/datocms-plugin-signifly-backups",
  "repository": {
    "type": "git",
    "url": "https://github.com/signifly/datocms-plugin-signifly-backups.git",
    "directory": "apps/plugin"
  },
  "license": "MIT",
  "author": {
    "name": "Signifly",
    "url": "https://signifly.com"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "backup",
    "environment",
    "automatic",
    "scheduled",
    "signifly"
  ],
  "datoCmsPlugin": {
