{
  "name": "@casperjuel/datocms-backup-api",
  "version": "1.0.4",
  "private": false,
  "description": "Backend API for DatoCMS automatic environment backups",
  "homepage": "https://github.com/signifly/datocms-plugin-signifly-backups",
  "repository": {
    "type": "git",
    "url": "https://github.com/signifly/datocms-plugin-signifly-backups.git",
    "directory": "apps/api"
  },
  "license": "MIT",
  "author": {
    "name": "Signifly",
    "url": "https://signifly.com"
  },
  "keywords": [
    "datocms",
    "backup",
    "api",
    "vercel",
    "signifly"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/lib/storage/kv.d.ts",
      "default": "./dist/lib/storage/kv.js"
    },
    "./auth": {
      "types": "./dist/lib/auth/verify.d.ts",
      "default": "./dist/lib/auth/verify.js"
    },
    "./datocms": {
      "types": "./dist/lib/datocms/index.d.ts",
      "default": "./dist/lib/datocms/index.js"
    },
    "./backup": {
      "types": "./dist/lib/backup/index.d.ts",
      "default": "./dist/lib/backup/index.js"
    },
    "./crypto": {
      "types": "./dist/lib/crypto/encryption.d.ts",
      "default": "./dist/lib/crypto/encryption.js"
    },
    "./validation": {
      "types": "./dist/lib/validation/index.d.ts",
      "default": "./dist/lib/validation/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "default": "./dist/middleware.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "npm run build:package && npm run build:next",
    "build:package": "tsc -p tsconfig.build.json",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build:package"
  },
  "dependencies": {
    "@datocms/cma-client": "^3.4.0",
    "@casperjuel/datocms-backup-shared": "^1.0.0",
    "@vercel/kv": "^3.0.0",
    "cron-parser": "^4.9.0",
    "uuid": "^11.0.4"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2"
  }
}
